{
  "name": "@48tools/website",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:api": "npm run clean && cross-env NODE_ENV=development gulp",
    "start:vercel": "vercel dev --listen 5054",
    "start:website": "cross-env NODE_ENV=development milktea-esm vite-start",
    "build:api": "npm run clean && cross-env NODE_ENV=production gulp",
    "build:website": "cross-env NODE_ENV=production milktea-esm vite-build",
    "build": "npm run clean && npm run build:api && npm run build:website",
    "clean": "rimraf dist/api dist/package.json dist/**/*.{html,css,js,map,svg,ico,jpg,png,webp,avif}"
  },
  "dependencies": {
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "got": "^12.1.0",
    "graphql": "^16.5.0"
  },
  "optionalDependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "antd-mobile": "^5.16.1",
    "antd-mobile-icons": "^0.3.0",
    "classnames": "^2.3.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "reselect": "^4.1.6"
  },
  "devDependencies": {
    "@sweet-milktea/milktea-cli": "^4.0.0",
    "@sweet-milktea/milktea-vite": "^4.0.0",
    "@sweet-milktea/server": "^4.0.0",
    "@types/express": "^4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.3",
    "gulp-plumber": "^1.2.1",
    "gulp-replace": "^1.1.3",
    "gulp-terser": "^2.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "vite-plugin-babel": "^1.1.0",
    "vite-plugin-html": "^3.2.0"
  }
}
