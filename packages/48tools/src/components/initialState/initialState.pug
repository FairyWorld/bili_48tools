// 从主线程获取的初始化的值
script.
  (function initialState() {
    const paramsResult = Array.from(new URLSearchParams(window.location.search).entries());

    globalThis.__INITIAL_STATE__ = paramsResult.reduce(function(result, [key, value]) {
      result[key] = value;

      return result;
    }, {});
  })();