{
  "name": "@48tools/dark-theme",
  "version": "3.10.2",
  "description": "编译48tools的暗黑模式css文件",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build": "rimraf ./dist && node src/index.js && npm run nano:dark",
    "nano:dark": "postcss dist/dark-theme.css > dist/dark-theme.min.css",
    "nano:light": "postcss dist/light-theme.css > dist/light-theme.min.css"
  },
  "dependencies": {
    "@sweet-milktea/utils": "^4.0.0",
    "cssnano": "^5.1.12",
    "glob": "^8.0.3",
    "less": "^4.1.3",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "rimraf": "*",
    "typescript": "*"
  }
}
